{
  "expo": {
    "name": "Kabelwerk Demo",
    "description": "Demo app showcasing the Kabelwerk SDK for React Native",
    "owner": "kabelwerk",
    "slug": "demo",
    "privacy": "public",
    "version": "0.0.0",
    "githubUrl": "https://github.com/kabelwerk/sdk-react-native",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
